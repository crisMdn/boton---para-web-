    "test": "echo \"No tests\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crisMdn/boton---para-web-.git"
  },
  "keywords": ["express", "api", "uploads", "multer", "postgres"],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/crisMdn/boton---para-web-/issues"
  },
  "homepage": "https://github.com/crisMdn/boton---para-web-#readme",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.890.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "multer": "^2.0.2",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  },
