{
  "name": "proyecto-botton",
  "version": "1.0.0",
  "description": "API modular para subir y listar imágenes (Express + Multer + Postgres)",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon --watch src --ext js,mjs,cjs,json src/server.js",
